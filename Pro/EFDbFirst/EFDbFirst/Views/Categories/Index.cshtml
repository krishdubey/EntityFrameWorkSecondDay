@model  IEnumerable<EFDbFirst.Models.tblCategory>

@{
    ViewBag.Title = "Index";
}

<h2>Categories Index</h2>
<ol>
    @foreach(var item in Model)
    {
       // <li>@item.CategoryName</li>
        <li>@Html.ActionLink(item.CategoryName,"Index","Products", new {id = item.CategoryId },null )</li>
    }
</ol>
